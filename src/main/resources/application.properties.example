# security
spring.security.user.name=your_user
spring.security.user.password=your_password
jwt.signing-secret=your_secret

# jwt
jwt.public.key=classpath:your_key_pub
jwt.private.key=classpath:your_app_key
jwt.issuer=your_issuer
jwt.ttl=your_ttl

jwt.cookie.access-token-name=your_access_token_name
jwt.cookie.refresh-token-name=your_refresh_token_name
jwt.refresh-token.ttl=your_refresh_token_ttl
jwt.cookie.secure=true

# flyway
spring.flyway.enabled=true
spring.flyway.baseline-on-migrate=true
spring.flyway.clean-on-validation-error=true
spring.flyway.clean-disabled=false

# database
spring.datasource.url=jdbc:postgresql://db:5432/your_db
spring.datasource.username=your_username
spring.datasource.password=your_password
spring.datasource.driverClassName=org.postgresql.Driver
spring.jpa.database-platform=org.hibernate.dialect.PostgreSQLDialect

# email sender
spring.mail.host=smtp.gmail.com
spring.mail.port=your_port
spring.mail.username=your_username
spring.mail.password=your_password
spring.mail.properties.mail.smtp.auth=true
spring.mail.properties.mail.smtp.starttls.enable=true